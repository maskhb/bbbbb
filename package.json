{
    "name": "react-framework-base",
    "version": "0.0.1",
    "description": "An out-of-box UI solution for enterprise applications",
    "private": true,
    "scripts": {
        "start": "cross-env ESLINT=none BABEL_CACHE=none roadhog dev",
        "start:no-proxy": "cross-env NO_PROXY=true ESLINT=none BABEL_CACHE=none roadhog dev",
        "build": "node ./tools/proxy && cross-env ESLINT=none BABEL_CACHE=none roadhog build",
        "build:no-tools": "cross-env ESLINT=none BABEL_CACHE=none roadhog build",
        "site": "roadhog-api-doc static && gh-pages -d dist",
        "analyze": "cross-env ANALYZE=true roadhog build",
        "lint:style": "stylelint \"src/**/*.less\" --syntax less",
        "lint": "eslint --ext .js src templates mock tests && npm run lint:style",
        "lint:fix": "eslint --fix --ext .js src templates mock tests && npm run lint:style",
        "test": "roadhog test",
        "test:component": "roadhog test ./src/components",
        "test:all": "node ./tests/run-tests.js",
        "sw2dts": "node ./tools/sw2dts.js"
    },
